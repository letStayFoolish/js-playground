  {
  "name": "js-myPlayground",
  "version": "1.0.0",
  "description": "A simple JavaScript playground for testing algorithms and code snippets",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "serve": "http-server -o",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.{js,json,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,json,html,css}\"",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "serve:dev": "webpack serve --mode development"
  },
  "private": true,
  "devDependencies": {
    "nodemon": "^2.0.22",
    "http-server": "^14.1.1",
    "prettier": "^3.0.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1",
    "core-js": "^3.32.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
